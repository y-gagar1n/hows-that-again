{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-transformer-remark": "^1.7.44",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rimraf public && rimraf .cache && gatsby build",
    "develop": "gatsby develop",
    "format": "rimraf public && rimraf .cache && prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "gh-pages-clean && rimraf public && rimraf .cache && gatsby build --prefix-paths && gh-pages -d public",
    "heroku-postbuild": "deploy"
  },
  "devDependencies": {
    "gatsby-plugin-sharp": "^2.0.12",
    "gatsby-remark-images": "^2.0.6",
    "gh-pages": "^1.2.0",
    "prettier": "^1.12.0",
    "rimraf": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
